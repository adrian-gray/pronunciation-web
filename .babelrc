{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": "last 2 versions"
      },
      "exclude": ["transform-async-to-generator"],
      "loose": true,
      "modules": "commonjs"
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "add-module-exports"
  ]
}
